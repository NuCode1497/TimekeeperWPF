<Application 
    x:Class="TimekeeperWPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="/Views/Main/MainWindow.xaml"
    DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--Remember: Order matters in XAML-->
                <ResourceDictionary x:Name="Theme" Source="Themes\GreenTheme.xaml"/>
                <ResourceDictionary Source="Calendar\CalendarStyles.xaml"/>
                <ResourceDictionary Source="Themes\Core.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
