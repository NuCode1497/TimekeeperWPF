    <SolidColorBrush x:Key="Highlight.Hover.BorderColor" Color="DarkCyan"/>
    <SolidColorBrush x:Key="Highlight.Hover.BackgroundColor" Color="Cyan"/>
    <clr:Double x:Key="Highlight.Hover.Opacity">0.8</clr:Double>
    <SolidColorBrush x:Key="Day.Highlight" Color="LightCyan"/>
    <SolidColorBrush x:Key="Day.Background" Color="MintCream"/>
    <clr:String x:Key="Day.WatermarkFormat" xml:space="preserve">ddd
MMM d</clr:String>
    <FontFamily x:Key="Day.FontFamily">"Segoe UI"</FontFamily>
    <StaticResource x:Key="Ayy.FontFamily" ResourceKey="Global.FontFamily"/>
    <SolidColorBrush x:Key="Day.WatermarkBrush" Color="DarkGray"/>
