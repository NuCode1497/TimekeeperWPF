    <SolidColorBrush x:Key="DataGrid.Background" Color="AliceBlue"/>
    <SolidColorBrush x:Key="EntityTag.Background" Color="Wheat"/>
    <SolidColorBrush x:Key="EntityTag.Background.Locked" Color="LightCoral"/>
    <SolidColorBrush x:Key="EntityTag.BorderBrush" Color="DarkSlateGray"/>
