        <GradientStop Color="MediumTurquoise" Offset="0.432"/>
        <GradientStop Color="Black" Offset="0.932"/>
        <GradientStop Color="PaleTurquoise" Offset="0.911"/>
    </RadialGradientBrush>
    <SolidColorBrush x:Key="TinyButton.Add.Foreground.Pressed" Color="Wheat"/>
    <SolidColorBrush x:Key="TinyButton.Add.Background.MouseOver" Color="Cyan"/>
    <SolidColorBrush x:Key="TinyButton.Add.Foreground.MouseOver" Color="Black"/>
