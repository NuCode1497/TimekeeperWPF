        <GradientStop Color="Green" Offset="0.432"/>
        <GradientStop Color="Black" Offset="0.932"/>
        <GradientStop Color="DarkGreen" Offset="0.911"/>
    </RadialGradientBrush>
    <SolidColorBrush x:Key="TinyButton.Add.Foreground.Static" Color="Wheat"/>
